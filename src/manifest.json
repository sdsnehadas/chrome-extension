{
  "manifest_version": 2,
  "name": "Life Extension",
  "short_name": "Make the most of a new tab",
  "description": "This extension helps you everywhere.",
  "version": "1.0.0",
  "author": "Sneha Das Sreejith @sdsnehadas",

  "icons": {
    "19": "assets/Icon-19.png",
    "38": "assets/Icon-38.png",
    "48": "assets/Icon-48.png"
  },

  "browser_action": {
    "default_icon": {
      "19": "assets/google_icon.png",
      "38": "assets/google_icon.png"
    },
    "default_title": "Open a new tab to view Life."
  },

  "chrome_url_overrides" : {
    "newtab": "index.html"
  },

  "background": {
    "page": "index.html",
    "persistent": false
  },

  "content_security_policy": "script-src https://code.jquery.com/jquery-3.1.1.slim.min.js https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.3/umd/popper.min.js https://cdnjs.cloudflare.com/ajax/libs/tether/1.4.0/js/tether.min.js https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-alpha.6/js/bootstrap.min.js ; default-src 'self'; object-src 'self'; style-src 'unsafe-inline' https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-alpha.6/css/bootstrap.min.css;",

  "permissions": ["activeTab"]
}


